{
  "version": "1.0.0-preview3-*",
  "dependencies": {
    "Microsoft.Extensions.CommandLineUtils.Sources": {
      "version": "1.1.0-*",
      "type": "build"
    }
  },
  "buildOptions": {
    "outputName": "ef",
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "keyFile": "../../tools/Key.snk"
  },
  "configurations": {
    "debug_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred",
        "define": [
          "DEBUG"
        ],
        "outputName": "ef.x86"
      }
    },
    "release_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred",
        "outputName": "ef.x86"
      }
    }
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      },
      "buildOptions": {
        "compile": {
          "include": [
          	"**/*.cs",
            "../Shared/CodeAnnotations.cs",
            "../Tools.Shared/CommandLineApplicationExtensions.cs",
            "../Tools.Shared/OperationHandlers.cs"
          ]
        }
      }
    },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.Extensions.DependencyModel": "1.0.1-*",
        "System.Runtime.Loader": "4.0.0",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "buildOptions": {
        "compile": {
          "include": [
          	"**/*.cs",
            "../Shared/CodeAnnotations.cs",
            "../Tools.Shared/CommandLineApplicationExtensions.cs"
          ]
        }
      }
    }
  }
}